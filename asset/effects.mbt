///|
pub fn spine_effects(wid : Int, n : Int) -> String {
  let s = Char::from_int(0x030A)
  let l = wid / n - 1
  let mut res = ""
  for i in 0..<n {
    res = res + "A" + mul_str(l, "\{s}")
  }
  res
}
